{
  "openapi": "3.1.0",
  "info": {
    "title": "SmithTech Eligibility and Systems API",
    "description": "API for handling systems, locations, charges, and various eligibility endpoints in SmithTech.",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "https://smithtech.io"
    }
  ],
  "paths": {
    "/api/systems": {
      "get": {
        "description": "Get a list of all systems",
        "operationId": "GetAllSystems",
        "responses": {
          "200": {
            "description": "A list of systems"
          }
        },
        "deprecated": false
      }
    },
    "/api/systems/{system_id}": {
      "get": {
        "description": "Get details of a specific system",
        "operationId": "GetSystemDetails",
        "parameters": [
          {
            "name": "system_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Details of the system"
          }
        },
        "deprecated": false
      }
    },
